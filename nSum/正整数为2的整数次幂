import java.util.Scanner;
public class Two {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        sc.close();
        boolean flag = isTwo(num);
        System.out.println(flag);
    }
    private static boolean isTwo(int num){
        return (num&(num-1))==0;
    }
}
